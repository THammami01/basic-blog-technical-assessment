  "engines": {
    "node": "18",
    "npm": ">=8",
    "yarn": ">=1"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "serve": "vite serve dist",
    "typecheck": "tsc --noEmit",
    "eslint:lint": "eslint . --max-warnings=0 --ext .ts,.tsx,.js,jsx",
    "eslint:fix": "eslint . --max-warnings=0 --ext .ts,.tsx,.js,jsx --fix",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write . --ignore-unknown",
    "clean": "rimraf node_modules dist",
    "hard-clean": "rimraf node_modules yarn.lock package-lock.json dist",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "yarn prettier:check"
